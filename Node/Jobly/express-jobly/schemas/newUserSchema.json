{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "username": "Kazuma",
            "password": "aquaisdumb",
            "first_name": "Kazuma",
            "last_name": "Satou",
            "email_handle": "Kazuma@neet.com",
            "photo_url": "http://www.jsonschema.net/url404",
            "is_admin": "false"
        }
    ],
    "required": [
        "username",
        "password",
        "first_name",
        "last_name",
        "email_handle",
        "photo_url",
        "is_admin"
    ],
    "properties": {
        "username": {
            "$id": "#/properties/username",
            "type": "string",
            "title": "The username schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Kazuma"
            ]
        },
        "password": {
            "$id": "#/properties/password",
            "type": "string",
            "title": "The password schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "aquaisdumb"
            ]
        },
        "first_name": {
            "$id": "#/properties/first_name",
            "type": "string",
            "title": "The first_name schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Kazuma"
            ]
        },
        "last_name": {
            "$id": "#/properties/last_name",
            "type": "string",
            "title": "The last_name schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Satou"
            ]
        },
        "email_handle": {
            "$id": "#/properties/email_handle",
            "type": "string",
            "title": "The email_handle schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Kazuma@neet.com"
            ]
        },
        "photo_url": {
            "$id": "#/properties/photo_url",
            "type": "string",
            "title": "The photo_url schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "http://www.jsonschema.net/url404"
            ]
        },
        "is_admin": {
            "$id": "#/properties/is_admin",
            "type": "string",
            "title": "The is_admin schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "false"
            ]
        }
    },
    "additionalProperties": true
}